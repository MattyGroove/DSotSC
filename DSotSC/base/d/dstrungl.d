BEGIN ~DSTRUNGL~

IF ~NumberOfTimesTalkedTo(0)~ THEN BEGIN 0
  SAY @1
  IF ~~ THEN REPLY @2 GOTO 1
  IF ~~ THEN REPLY @3 GOTO 3
END

IF ~~ THEN BEGIN 1
  SAY @4
  IF ~~ THEN REPLY @5 GOTO 2
  IF ~~ THEN REPLY @6 GOTO 6
END

IF ~~ THEN BEGIN 2
  SAY @7
  IF ~~ THEN DO ~StartStore("DStrungl",LastTalkedToBy(Myself))~ EXIT
END

IF ~~ THEN BEGIN 3
  SAY @8
  IF ~~ THEN EXIT
END

IF ~True()~ THEN BEGIN 4
  SAY @9
  IF ~~ THEN DO ~StartStore("DStrungl",LastTalkedToBy(Myself))~ EXIT
END

IF WEIGHT #0 ~StateCheck(Myself,STATE_CHARMED)~ THEN BEGIN 5
  SAY @10
  IF ~~ THEN EXIT
END

IF ~~ THEN BEGIN 6
  SAY @11
  IF ~~ THEN EXIT
END
